{
  "name": "@skillcoop/api",
  "description": "Api for Skillcoop",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "vitest",
    "clean:build": "rimraf ./dist",
    "build": "pnpm run clean:build && pnpm run compile",
    "compile": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "ts-coverage": "typescript-coverage-report",
    "lint": "eslint . index.ts --fix",
    "dev": "nodemon --exec ts-node-transpile-only ./index.ts",
    "start": "nodemon dist/index.js",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed:prod": "node dist/prisma/seed.js",
    "db:seed:dev": "ts-node-transpile-only prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@skillcoop/date-handler": "workspace:^",
    "@skillcoop/schema": "workspace:^",
    "@skillcoop/types": "workspace:^",
    "nodemailer": "^6.9.7",
    "nodemon": "^3.0.1",
    "prisma": "^5.5.2",
    "prisma-kysely": "^1.7.1",
    "qs": "^6.11.2",
    "randombytes": "^2.1.0",
    "sharp": "^0.32.6",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "@aws-sdk/client-cloudfront": "^3.454.0",
    "@aws-sdk/client-s3": "^3.441.0",
    "@aws-sdk/s3-request-presigner": "^3.441.0",
    "@prisma/client": "5.5.2",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint-config": "workspace:^",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "i18next": "^23.7.6",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.26.3",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "multer-s3": "^3.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/bcrypt": "^5.0.1",
    "@types/better-sqlite3": "^7.6.6",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/express-session": "^1.17.9",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.9",
    "@types/multer-s3": "^3.0.2",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.13",
    "@types/qs": "^6.9.9",
    "@types/randombytes": "^2.0.2",
    "rimraf": "^5.0.5",
    "prisma": "^5.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
